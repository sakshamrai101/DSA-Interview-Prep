class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if s is None or t is None:
            return False
        
        if len(s) != len(t):
            return False

        countS = {}
        countT = {}
        
        for c in s:
            if c in countS:
                countS[c] += 1
            else:
                countS[c] = 1
        
        for c in t:
            if c in countT:
                countT[c] += 1
            else:
                countT[c] = 1

        for char in countS:
            if countS[char] != countT.get(char, 0):
                return False
        
        return True 

# Example usage
solution = Solution()
print(solution.isAnagram("anagram", "nagaram"))  # Should return True
